{
  "name": "m2i",
  "version": "0.3.1",
  "description": "A minimalist markdown to image converter.",
  "keywords": [
    "m2i",
    "typescript",
    "cli",
    "zce"
  ],
  "repository": "zce/m2i",
  "license": "MIT",
  "author": "zce <w@zce.me> (https://zce.me)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "bin/m2i.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "ts-standard --fix",
    "compile": "tsc",
    "build": "npm run clean && npm run lint && npm run compile",
    "test": "jest --coverage",
    "prepare": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "cac": "^6.7.1",
    "cosmiconfig": "^7.0.0",
    "marked": "^1.2.9",
    "ora": "^5.3.0",
    "puppeteer": "^7.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/marked": "1.2.2",
    "@types/node": "14.14.25",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-standard": "10.0.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 10000
  },
  "renovate": {
    "extends": [
      "zce"
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
